        {
          "commands": [
            "bash",
            "curl -o- https://raw.githubusercontent.com/nvm-sh/nvm/v0.40.1/install.sh | bash",
            "nvm use 18",
            "chmod +x ./official_joke_api-master/scripts/setup-db.sh",
